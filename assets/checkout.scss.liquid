.ico-mglass,.index .ico-mglass,body.black-bg .top-menu .ico-mglass {
  position:relative;
  display:inline-block;
  background: transparent;
  border-radius: 30px;
  height: 8px;
  width: 8px;
  border: 2px solid #fff;
  &:after {
    content: "";
    height: 2px;
    width: 6px;
    background: #fff;
    position:absolute;
    top:10px;
    left:7.5px;
    -webkit-transform: rotate(45deg);
     -moz-transform: rotate(45deg);
      -ms-transform: rotate(45deg);
       -o-transform: rotate(45deg);
  }
  
}

.page .ico-mglass,  [class^='collection'] .ico-mglass, [class^='blog-']  .ico-mglass ,
[class*='blog-'] .ico-mglass,.search .ico-mglass,[class*='product'] .ico-mglass,.search .ico-mglass {
  position:relative;
  display:inline-block;
  background: transparent;
  border-radius: 30px;
  height: 8px;
  width: 8px;
  border: 2px solid #000;
  &:after {
    content: "";
    height: 2px;
    width: 6px;
    background: #000;
    position:absolute;
    top:10px;
    left:7.5px;
    -webkit-transform: rotate(45deg);
     -moz-transform: rotate(45deg);
      -ms-transform: rotate(45deg);
       -o-transform: rotate(45deg);
  }
}

h1 a, h2 a, h3 a, h4 a, h5 a, h6 a, .collection_title { font-weight: inherit; }
h1 a, h1 a:visited, .collection_title a, .collection_title a:visited { color: {{ settings.headline_color }} }
h1 a:hover, h1 a:active, .collection_title a:hover, .collection_title a:active { color: {{ settings.link_color }} }
h1, .h1, h1.home, h2.title, .collection_title, h3.title { font-size: {{ settings.headline_font_size }}px; line-height: 1em; margin: 0 auto 15px; clear:both; font-weight: {{ settings.headline_weight }}; padding-top: 4px; }
h1.product_name { margin-bottom: 15px; }
h1.article_title { margin-bottom: 5px; }
h1.slogan{font-family:'HelveticaNeueLTStd-Md', Helvetica, Arial, sans-serif;text-transform: none;font-size: 12px;color: #fff;letter-spacing: 0;margin: 0;@media all and (max-width: 767px) {font-size:11px;}}
h2.product_name a { color: {{ settings.headline_color}}; }
h2, .h2 {font-family:'HelveticaNeueLTStd-LtEx', Helvetica, Arial, sans-serif; font-size: 20px; margin-bottom: 0.75em; line-height: 1em;letter-spacing:1px; }
.dark{color:#333;}
.light{color:#fff;}
.bold{font-family:'HelveticaNeueLTStd-Bd', Helvetica, Arial, sans-serif;}
.medium{font-family:'HelveticaNeueLTStd-Md, Helvetica, Arial, sans-serif';}
h1 strong,  h2 strong,  h3 strong,  h4 strong,  h5 strong,  h6 strong{text-transform:uppercase;}
h3, .h3 { font-size: 14px; text-transform:none; font-style:normal; line-height: 28px; margin: 0 auto 15px 0;font-family:'HelveticaNeueLTStd-LtEx'; }
h3.title a { color: {{ settings.headline_color }}; }
h1.collection_title, div.collection_title { margin-bottom: 0; line-height: {{ settings.headline_font_size }}px; display: inline; }
h1.collection_title_tags, div.collection_title_tags { padding-right: 25px; border-right: solid 1px {{ settings.border_color }}; }
h4, .h4, h4.title a, h6.title {font-family:'HelveticaNeueLTStd-Bd', Helvetica, Arial, sans-serif;font-size: 14px;line-height: 1em;letter-spacing:3px;padding: 0;padding: 7px 0;color: {{ settings.headline_color }};}
h4.title a { border: 0; padding: 0; margin: 0;}
h5, .h5 { font-size: 14px; padding-bottom: 1em;line-height:2em;font-family:'HelveticaNeueLTStd-Md', Helvetica, Arial, sans-serif; }
h5.sub_title { padding: 5px 0; color: {{ settings.headline_color }} }
h5.sub_title a { color: {{ settings.headline_color }} }
.subheader { color: #777; }
p{font-family: 'HelveticaNeueLTStd-Lt', 'Helvetica Neue Light', Helvetica, Arial, sans-serif;margin: 0 0 15px; font-style: normal;font-size: 14px;font-style: normal;font-variant: normal;letter-spacing:1px;line-height: 28px;@media all and (max-width: 767px) {padding:0 10px;}}
p.p1{}
p.p2,.p2{font-family: 'HelveticaNeueLTStd-Lt', 'Helvetica Neue Light', Helvetica, Arial, sans-serif;font-size: 16px;font-style: normal;font-variant: normal;line-height: 1.75em;letter-spacing:0.75px;}
p.p3,.p3{font-family:'HelveticaNeueLTStd-LtEx', Helvetica, Arial, sans-serif; font-size:14px;}
p.p4,.p4,.breadcrumb a,.breadcrumb span{font-family: 'HelveticaNeueLTStd-Lt', Helvetica, Arial, sans-serif;font-size: 12px;font-style: normal;line-height: 12px;} 
p.p5,.p5{font-family: 'HelveticaNeueLTStd-Lt', 'Helvetica Neue Light', Helvetica, Arial, sans-serif;font-size: 11px;font-style: normal;font-variant: normal;line-height: 11px;} 
h6, .h6, h6.title, ul.footer_menu a, ul.footer_menu a:visited,p.p6,.p6,.tags a{font-family: 'HelveticaNeueLTStd-Roman', Helvetica, Arial, sans-serif;font-size: 11px;font-style: normal;font-variant: normal;line-height: 11px;margin-bottom:5px;}
.swatch .p6.option_title{font-family: 'HelveticaNeueLTStd-Md', Helvetica, Arial, sans-serif;font-weight:bold;}
.swatch{margin-top:10px;}
.swatch:nth-child(2){margin-top:15px;}


